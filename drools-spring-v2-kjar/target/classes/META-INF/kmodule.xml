<?xml version="1.0" encoding="UTF-8"?>
<kmodule xmlns="http://jboss.org/kie/6.0.0/kmodule"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<kbase name="rules" packages="rules">
		<ksession name="rulesSession" />
	</kbase>
	<kbase name="patient" packages="patient">
		<ksession name="patientSession" />
	</kbase>
	<kbase name="count" packages="count">
		<ksession name="countSession" />
	</kbase>
	<kbase name="report" packages="report">
		<ksession name="reportSession" />
	</kbase>
	<kbase name="monitoring" eventProcessingMode="stream"
		packages="monitoring">
		<ksession name="monitoringSession" clockType="pseudo" />
	</kbase>
</kmodule>
